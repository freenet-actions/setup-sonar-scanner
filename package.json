    "@types/node": "14.14.22",
    "@typescript-eslint/parser": "4.14.1",
    "@vercel/ncc": "0.28.6",
    "eslint": "7.19.0",
    "eslint-plugin-github": "4.1.1",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "jest-circus": "27.0.4",
